class Array{
public static void main(String arr[]){
int a[][]={{1,2},{4,5}};
int b[][]={{1,2},{4,5}};
int c[][]=new int[2][2];
for(int i=0;i<2;i++){
for(int j=0;j<2;j++){
for(int k=0;k<2;k++){
c[i][k]+=a[i][j]*b[j][k];
}
}
}
for(int i=0;i<2;i++){
for(int j=0;j<2;j++){
System.out.print(c[i][j]+" ");
}
System.out.println(" ");
}
}
}
