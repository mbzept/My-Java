class Parent
{
int i=10;
String n="Sec D";
void money()
{
System.out.println("Millions");
}
}
-------------------------------------------------------------------------------------------------------------------------------
class Child extends Parent
{
void earning()
{
System.out.println("in earning");
}

public static void main(String a[])
{
Child c=new Child();
c.earning();
c.money();
System.out.println(c.i);
System.out.println(c.n);
}
}
